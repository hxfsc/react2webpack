{
    "presets": [
        "env", ["es2015", {
            "loose": true
        }],
        "stage-1",
        "react"
    ],

    "plugins": [
        ["import", {
            "libraryName": "antd",
            "style": "css"
        }],
        ["react-hot-loader/babel"],
        ["transform-decorators-legacy"],
        ["transform-class-properties"]
    ]
}
